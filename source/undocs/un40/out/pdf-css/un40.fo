                                        <fo:inline font-family="monospace" font-size="0.9em" role="Code">/usr/local/unravel/etc/unravel.properties</fo:inline>
                                    </fo:inline> with <fo:inline font-family="monospace" font-size="0.9em" role="Code">vi</fo:inline>.</fo:block></fo:block-container><fo:block-container background-color="rgb(240 , 240 , 240)" font-family="monospace" font-size="0.9em" margin-bottom="0.5em" margin-left="0" margin-right="0.5em" margin-top="0.5em" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code">sudo vi /usr/local/unravel/etc/unravel.properties</fo:block></fo:block-container></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
