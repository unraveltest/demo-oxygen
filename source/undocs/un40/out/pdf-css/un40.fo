                <fo:block-container id="d33e13293" border-bottom-width="2pt" border-bottom-style="none" font-family="Calibri, UnDotum, sans-serif" font-size="1.5em" font-weight="bold" margin-bottom="0.2em" margin-top="0.6em" padding-bottom="1pt" page-break-after="avoid"><fo:block end-indent="0" start-indent="0" role="H3">Part
                    2: Enable Additional Data Collection / Instrumentation for HDP </fo:block></fo:block-container>
                <fo:block-container margin-bottom="0" margin-left="0" margin-right="0" margin-top="0"><fo:block end-indent="0" start-indent="0">
                    <fo:block-container margin-bottom="0.5em" margin-left="0" margin-right="0" margin-top="0.5em"><fo:block end-indent="0" start-indent="0" role="Sect"><fo:inline font-weight="bold"/>
                        <fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="0.25in" margin-bottom="0.5em" margin-left="0" margin-right="0" margin-top="0.5em">
                            <fo:list-item margin-bottom="0.2em"><fo:list-item-label end-indent="label-end()"><fo:block text-align="center" font-weight="bold">1.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
