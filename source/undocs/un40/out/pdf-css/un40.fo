                        <fo:block-container margin-bottom="0.5em" margin-left="0" margin-right="0" margin-top="0.5em"><fo:block end-indent="0" start-indent="0">
                            <fo:inline font-style="italic">Unravel Web UI
                                Login Screen</fo:inline>
                        </fo:block></fo:block-container>
                        <fo:block-container margin-bottom="0.5em" margin-left="0" margin-right="0" margin-top="0.5em"><fo:block end-indent="0" start-indent="0">Congratulations!</fo:block></fo:block-container>
                        <fo:block-container margin-bottom="0.5em" margin-left="0" margin-right="0" margin-top="0.5em"><fo:block end-indent="0" start-indent="0">Unravel Server is up and running. You should
                            begin to see information collected. For instructions on using Unravel Web UI, see the <fo:inline color="navy"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_7">User
                                Guide</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_7"><fo:inline font-style="italic"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_7"> (page </fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_7"><fo:page-number-citation ref-id="unique_7"/></fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_7">)</fo:basic-link></fo:inline></fo:basic-link></fo:inline>.</fo:block></fo:block-container>
