aws s3 cp s3://unraveldata-clients/unravel_emr_setup.sh . 
chmod +x unravel_emr_setup.sh 
./unravel_emr_setup.sh --unravel-server $LOCAL_IP:3000</fo:block></fo:block-container>
                                <fo:inline/>
