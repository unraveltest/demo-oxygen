                        <fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="0.25in" margin-bottom="0.5em" margin-left="0" margin-right="0" margin-top="0.5em">
                            <fo:list-item margin-bottom="0.2em"><fo:list-item-label end-indent="label-end()"><fo:block text-align="center" font-weight="bold">1.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0"><fo:block-container margin-bottom="0.5em" margin-left="0" margin-right="0" margin-top="0.5em"><fo:block end-indent="0" start-indent="0">
                                    <fo:inline>
                                        <fo:inline>
                                            <fo:inline>Enable profiling:</fo:inline>
                                        </fo:inline>
                                    </fo:inline>
                                </fo:block></fo:block-container><fo:block-container background-color="rgb(240 , 240 , 240)" font-family="monospace" font-size="0.9em" margin-bottom="0.5em" margin-left="0" margin-right="0.5em" margin-top="0.5em" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code">set mapreduce.task.profile=true;</fo:block></fo:block-container></fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                            <fo:list-item margin-bottom="0.2em"><fo:list-item-label end-indent="label-end()"><fo:block text-align="center" font-weight="bold">2.
