            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
                            <fo:inline>
                                <fo:inline color="navy"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_35">Advanced
                                    Topics</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_35"><fo:inline font-style="italic"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_35"> (page </fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_35"><fo:page-number-citation ref-id="unique_35"/></fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_35">)</fo:basic-link></fo:inline></fo:basic-link></fo:inline>
