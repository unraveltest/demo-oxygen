                     <entry class="- topic/entry " colname="col2" dita-ot:x="2" dita-ot:y="1" outputclass="confluenceTd" xtrc="entry:2;67:56" xtrf="file:/C:/Users/five/Documents/unravel/demos/Oxygen/git/demo-oxygen/source/undocs/un40/Setting-Up-Email-for-Auto-Actions-and-Collaboration_51162243.dita">mail.smtp.port</entry>

                     <entry class="- topic/entry " colname="col3" dita-ot:x="3" dita-ot:y="1" outputclass="confluenceTd" xtrc="entry:3;69:56" xtrf="file:/C:/Users/five/Documents/unravel/demos/Oxygen/git/demo-oxygen/source/undocs/un40/Setting-Up-Email-for-Auto-Actions-and-Collaboration_51162243.dita">25</entry>

