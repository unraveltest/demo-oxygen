                  <row class="- topic/row ">

                     <entry class="- topic/entry " colname="col1" dita-ot:morecols="0" dita-ot:x="1" dita-ot:y="2" nameend="col1" namest="col1" outputclass="confluenceTd" xtrc="entry:5;76:85" xtrf="file:/C:/Users/five/Documents/unravel/demos/Oxygen/git/demo-oxygen/source/undocs/un40/Setting-Up-Email-for-Auto-Actions-and-Collaboration_51162243.dita" colspan="1">AUTHENTICATE</entry>

