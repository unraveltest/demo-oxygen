                  </row>
                  <row class="- topic/row ">

                     <entry class="- topic/entry " colname="col1" dita-ot:morecols="0" dita-ot:x="1" dita-ot:y="3" nameend="col1" namest="col1" outputclass="confluenceTd" xtrc="entry:9;90:85" xtrf="file:/C:/Users/five/Documents/unravel/demos/Oxygen/git/demo-oxygen/source/undocs/un40/Setting-Up-Email-for-Auto-Actions-and-Collaboration_51162243.dita" colspan="1">START TLS</entry>

                     <entry class="- topic/entry " colname="col2" dita-ot:x="2" dita-ot:y="3" outputclass="confluenceTd" xtrc="entry:10;92:56" xtrf="file:/C:/Users/five/Documents/unravel/demos/Oxygen/git/demo-oxygen/source/undocs/un40/Setting-Up-Email-for-Auto-Actions-and-Collaboration_51162243.dita">mail.smtp.starttls.enable</entry>

                     <entry class="- topic/entry " colname="col3" dita-ot:x="3" dita-ot:y="3" outputclass="confluenceTd" xtrc="entry:11;94:56" xtrf="file:/C:/Users/five/Documents/unravel/demos/Oxygen/git/demo-oxygen/source/undocs/un40/Setting-Up-Email-for-Auto-Actions-and-Collaboration_51162243.dita">false</entry>

