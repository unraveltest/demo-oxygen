         <p class="- topic/p " xtrf="file:/C:/Users/five/Documents/unravel/demos/Oxygen/git/demo-oxygen/source/undocs/un40/50688326.dita" xtrc="p:15;136:13">
            <image href="images/icons/emoticons/lightbulb_on.png" outputclass="emoticon emoticon-light-on" placement="inline" class="- topic/image " xtrf="file:/C:/Users/five/Documents/unravel/demos/Oxygen/git/demo-oxygen/source/undocs/un40/50688326.dita" xtrc="image:1;138:57">
